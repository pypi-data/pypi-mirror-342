  rate_limit:
    role-based:
      config:
        ## 是否开启限流策略 true:开启,false:不开启
        enable: true
        ## 限流周期时间(秒)
        time_range: 600
        ## Redis记录缓存时间秒
        cache_time_range: 1000
        ## 白名单userId列表
        user-white-list:
        ## 黑名单userId列表
        user-black-list:
        ## 游客白名单UserId列表（暂不使用）
        guest-white-list:
        ## 游客黑名单UserId列表（暂不使用）
        guest-black-list:
        ## 角色限流策略配置
        role-config-mapping:
          ##游客限流策略（暂不使用）
          GUEST:
            ##是否使用限流策略，默认不启用
            enable: false
            ##周期内同时访问用户量 0:不限制, >0:限制最大量 <0:全部限制访问
            limit: 0
          ##普通用户限流策略（暂不使用）  
          NORMAL_USER:
            ##是否使用限流策略，默认不启用
            enable: false
            ##周期内同时访问用户量 0:不限制, >0:限制最大量 <0:全部限制访问
            limit: 0
          ##会写邀请用户限流策略  
          HX_INVITE_ROLE:
            ##是否使用限流策略，默认启用
            enable: true
            ##周期内同时访问用户量 0:不限制, >0:限制最大量 <0:全部限制访问
            limit: 1000
          ##会写权益包用户限流策略    
          HX_ROLE:
            ##是否使用限流策略，默认启用
            enable: true
            ##周期内同时访问用户量 0:不限制, >0:限制最大量 <0:全部限制访问
            limit: 10000
