    config: function (options) {
        if (typeof options == undefined) {
            return false;
        }
        if (options.pe && options.pe.length > 0) {
            this._parentElmentId = options.pe;
        }
        if (options.zt && options.zt.length > 0) {
            // var currZTCodes = options.zt;
            // if (currZTCodes) {
            //     this.tpcode = currZTCodes.split(';')[0];
            // }
            this._ztCode = options.zt;
        }
        if (options.adpot && options.adpot.length > 0) {
            this.adpotCode = options.adpot.split(';')[0];
        }
        if (options.w && options.w.length > 0) {
            if (options.w.length > 1) {
                this._width = options.w;
            } else if (options.w == "1") {
                this._use_server_width = true;
            }
        }
        if (options.h && options.h.length > 0) {
            if (options.h.length > 1) {
                this._height = options.h;
            } else if (options.h == "1") {
                this._use_server_heigth = true;
            }
        }
        if (options.s && options.s.length > 0 && !isNaN(parseInt(options.s))) {
            var sizeValueNumber = parseInt(options.s);
            if (sizeValueNumber < 20) {
                this.adpotSize = sizeValueNumber;
            }
        }
        if (options.wcss && options.wcss.length > 0) {
            this._writeCssType = options.wcss;
        }
        var tpkw = "";
        if (options.kw && options.kw.length > 0) {
            tpkw = options.kw.replace(/(<([^>]+)>)/ig, "").replace(/\ +/g, "").replace(/[\r\n]/g, "");
        } else {
            //tpkw = $(".row .keywords").text() == undefined ? "" : $(".row .keywords").text();
        }
        this._kw = decodeURIComponent(tpkw);
        if (options.maga && options.maga.length > 0) {
            this._maga = options.maga;
        }
        if (options.name && options.name.length > 0) {
            this._name = decodeURIComponent(options.name);
        }
        if (options.apiurl && options.apiurl.length > 10) {
            this.apiUrl = decodeURIComponent(options.apiurl);
        }
        if (options.enableclose && 'false' == options.enableclose) {
            this.enableClose = false;
        }
        if (options.intertime != undefined && options.intertime != null) {
            var intertime = parseInt(options.intertime);
            if (!isNaN(intertime)) {
                this._rotationIntervalTime = intertime;
            }
        }
        this.parsetIEBrowser();
        this.initRecommendId();
        this.addcss();
        this.intervalLoadAds();
    },
    RenderAdHtml: function (adsHtml, parentElmentId) {
        //使用正则，替换多次
        var reg1 = new RegExp('{#1}', "g");
        var isUseWrite = true;
        if (parentElmentId.length > 0) {
            isUseWrite = false;
            var parentElment = document.getElementById(parentElmentId);
            if (parentElment) {
                $("#" + parentElmentId).append(adsHtml.replace(reg1, '\n'));
            }
        }
        if (isUseWrite) {
            document.write(adsHtml.replace(reg1, '\n'));
        }
    },
    init: function (obj) {
        var that = this;
        if (obj == null || typeof(obj.attr) != 'function') {
            return false;
        }
        var carousel = document.getElementById(obj.attr("id"));
        if (carousel == null || typeof (carousel.querySelector) != 'function') {
            return false;
        }
        var carouselInner = carousel.querySelector('.carousel-inner');
        var carouselItems = carousel.querySelectorAll('.carousel-item');
        if (carouselInner == null || carouselItems == null) {
            return false;
        }
        var prevBtn = carousel.querySelector('.adbutprev-btn');
        var nextBtn = carousel.querySelector('.adbutnext-btn');
        var closeBtn = carousel.querySelector('.adbutclose a');
        var carouselDots = carousel.querySelector('.carousel-dots');
        var carouselDotItems = [];

        // Initialize carousel
        var currentSlide = 0;
        if (that._browser.msie && that._browser.version < 11) {
            for (var i = 0; i < carouselItems.length; i++) {
                carouselItems[i].style.width = carousel.style.width;
            }
            carouselInner.style.width = carouselItems.length * 100 + '%';
            carouselInner.style.left = '-' + currentSlide * 100 + '%';
        }
        function showSlide(slideIndex) {
            if (slideIndex < 0) {
                slideIndex = carouselItems.length - 1;
            } else if (slideIndex > carouselItems.length - 1) {
                slideIndex = 0;
            } else {
                currentSlide = slideIndex;
            }
            currentSlide = slideIndex;
            if (that._browser.msie && that._browser.version < 11) {
                carouselInner.style.left = '-' + currentSlide * 100 + '%';
            } else {
                carouselInner.style.transform = 'translateX(-' + currentSlide * 100 + '%)';
            }
            // update close tip 
            that.resetAdCloseTip(carouselItems[slideIndex].getAttribute("data-tipicon"));
            // Update active dot
            for (var i = 0; i < carouselDotItems.length; i++) {
                carouselDotItems[i].classList.remove("active");
                if (i === currentSlide) {
                    carouselDotItems[i].classList.add("active");
                }
            }
            that.WirteShowLog();
        }

        function nextSlide() {
            if ($(that._obj).is(':visible')) {
                showSlide(currentSlide + 1);
            }
        }

        // Render dots
        if (carouselItems.length > 1) {
            for (var i = 0; i < carouselItems.length; i++) {
                var dot = document.createElement('span');
                dot.classList.add('carousel-dot');
                dot.setAttribute("id", "dot" + i);
                dot.setAttribute("data-index", + i);
                dot.addEventListener('click', function () {
                    var slideIndexTemp = parseInt($(this).attr("data-index"));
                    showSlide(slideIndexTemp);
                });
                carouselDotItems.push(dot);
                if (carouselDots != null) {
                    carouselDots.appendChild(dot);
                }
            }
            if(carousel != null){
                carousel.addEventListener('mouseover', function(){
                    if(prevBtn != null){
                        prevBtn.style.display = 'block';
                    }
                    if (nextBtn != null) {
                        nextBtn.style.display = 'block';
                    }
                });
                carousel.addEventListener('mouseout', function(){
                    if(prevBtn != null){
                        prevBtn.style.display = 'none';
                    }
                    if (nextBtn != null) {
                        nextBtn.style.display = 'none';
                    }
                });
            }
        }

        showSlide(currentSlide);

        if (prevBtn != null) {
            prevBtn.addEventListener('click', function () {
                showSlide(currentSlide - 1);
            });
        }
        if (nextBtn != null) {
            nextBtn.addEventListener('click', nextSlide);
        }
        // window.addEventListener('resize', function () {
        // Adjust carousel item dimensions according to screen size
        // var itemWidth = carousel.offsetWidth;
        // var itemHeight = carousel.offsetHeight;
        // carouselItems.forEach(function (item) {
