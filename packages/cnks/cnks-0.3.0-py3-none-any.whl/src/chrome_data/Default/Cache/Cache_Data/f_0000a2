    includeCss: function (url) {
        var head = document.getElementsByTagName("head")[0];
        var link = document.createElement("link");
        link.href = url;
        link.rel = "stylesheet";
        link.type = "text/css";
        head.appendChild(link)
    },
    includeJs: function (url) {
        var head = document.getElementsByTagName("head")[0];
        var script = document.createElement("script");
        script.src = url;
        script.type = "text/javascript";
        head.appendChild(script)
    },
    resetAdCloseTip: function (tipicon) {
        if (tipicon == undefined || $.trim(tipicon).length == 0) {
            $("#" + this._recommendId + " .adbutclose").hide();
        } else {
            $("#" + this._recommendId + " .adbutclose a").html(tipicon + this.closeTipXText);
            $("#" + this._recommendId + " .adbutclose").show();
        }
    },
    createAdMainItemsContent: function (adlist) {
        var html = "";
        if (adlist != null && adlist.length > 0) {
            if (adlist.length == 1) {
                html += this.createAdMainItem(adlist[0]);
            } else {
                var sortedArr = adlist.slice();
                this.randomSort(sortedArr);
                for (var i = 0; i < sortedArr.length; i++) {
                    html += this.createAdMainItem(sortedArr[i]);
                }
            }
        }
        return html;
    },
    createAdMainItem: function (adobj) {
        var tipicon = '';
        if (adobj.tipIcon != undefined) {
            tipicon = $.trim(adobj.tipIcon);
        }
        var html = "<div class=\"carousel-item\" data-tipicon=\"" + tipicon + "\">";
        if (adobj.clickUrl && adobj.clickUrl.length > 5) {
            html += "<a class=\"but-ad-item-click\" href=\"" + adobj.clickUrl + "\" target=\"_blank\" dataid=\"" + adobj.workerId + "\" datatitle=\"" + adobj.workerId + "\">";
        } else {
            html += "<a class=\"but-ad-item-click\" dataid=\"" + adobj.workerId + "\" datatitle=\"" + adobj.workerId + "\">";
        }
        html += "<img src=\"" + adobj.fileName + "\" alt=\"" + adobj.workerId + "\" isshowfont=\"0\" >";
        html += "</a>";
        html += "</div>";
        return html;
    },
    createAdTotalContent: function (objadList) {
        if (objadList == null || objadList.length == 0) {
            return '';
        }
        this.maxExposure = objadList.length == 1 ? 1 : objadList.length * this.maxExposureRound;
        var tipiconText = '';
        var closeTipStyle = '';
        if (objadList[0].tipIcon != undefined) {
            var tipicon = $.trim(objadList[0].tipIcon);
            if (tipicon.length == 0) {
                closeTipStyle = "style='display:none;'";
            } else {
                tipiconText = tipicon + this.closeTipXText;
            }
        } else {
            tipiconText = "广告" + this.closeTipXText;
        }
        var html = "<div id=\"carousel\" class=\"carousel\">";
        if (this.enableClose) {
            html += "<div class=\"adbutclose\" " + closeTipStyle + "><a title=\"关闭\">" + tipiconText + "</a><div></div></div>";
        } else {
            html += "<div class=\"adbutclose\" " + closeTipStyle + "><a>" + tipiconText + "</a><div></div></div>";
        }
        html += "<div class=\"carousel-inner\">";
        html += this.createAdMainItemsContent(objadList);
        html += "</div>";
        html += "<div class=\"carousel-dots\"></div>";
        html += "<a class=\"adbutprev-btn\"></a>";
        html += "<a class=\"adbutnext-btn\"></a>";
        html += "</div>";
        html += "</div>";
        return html;
    },
    randomSort: function (arr) {
        for (var i = arr.length - 1; i > 0; i--) {
            var j = Math.floor(Math.random() * (i + 1));
            //[arr[i], arr[j]] = [arr[j], arr[i]];
            var temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
    },
    consoleLog: function (msg) {
        try {
            console.log(msg);
        } catch (e) { }
    },
    initRecommendId: function () {
        var timestamp = Date.parse(new Date());
        var adDivId = "recommend" + timestamp + Math.round(Math.random() * 100000);
        this._recommendId = adDivId;
    },
    intervalLoadAds: function () {
        if (this._parentElmentId && this._parentElmentId.length > 0) {
            var that = this
            var timer = setInterval(function () {
                var target = document.getElementById(that._parentElmentId)
                // that.consoleLog(target)
                if (target) {
                    clearInterval(timer)
                    that.loadAds()
                }
            }, 1000)
        } else {
            this.loadAds()
        }
    },
    resetSizeUseData: function (parentObj, data) {
        var that = this;
        if (that._use_server_width || that._use_server_heigth) {
            if (data.data == null || data.data.length == 0) {
                return;
            }
            var firstItem = data.data[0];
            if (that._use_server_width && firstItem.width && firstItem.width.length > 0) {
                $(parentObj).css({ "width": firstItem.width + "px" });
            }
            if (that._use_server_heigth && firstItem.height && firstItem.height.length > 0) {
                $(parentObj).css({ "height": firstItem.height + "px" });
            }
        }
    },
    loadAds: function () {
        var that = this;
        // var timestamp = Date.parse(new Date());
        // var adDivId = "recommend" + Math.round(Math.random() * 100000);
        var adDivId = this._recommendId;
        this.RenderAdHtml("<div id=\"" + adDivId + "\"></div>", this._parentElmentId);
        var _obj = $("#" + adDivId, window.parent.document).length == 1 ? $("#" + adDivId, window.parent.document) : $("#" + adDivId);
        if (_obj && _obj.length == 0) {
            return;
        }
        this._obj = _obj;
        if (that.cache("ad_close") == 1) {
            $(_obj).remove();
            return
        }
        if (this._width > 0) {
            $(_obj).css({ "width": this._width + "px", "height": this._height + "px", "margin": "2px 0" });
        } else {
            $(_obj).css({ "height": this._height + "px", "margin": "2px 0" });
        }

        tpcode = this._ztCode;
        //测试
        //tpcode = "E055";
        var that = this;

        $.ajax({
            url: this.apiUrl + "app/get/ads",
            type: "GET",
            // dataType: "jsonp",
            crossDomain: true,
            xhrFields: {
                widthCreate: true,
            },
            data: {
                kw: this._kw,
                name: this._name,
                maga: this._maga,
                size: this.adpotSize,
                spotcode: this.adpotCode,
                subject: tpcode
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $(that._obj).remove()
            },
            success: function (data) {
                //that.consoleLog(data);
                try {
                    $(_obj).html("");
                    if (data.code == undefined || data.code != 1 || data.data == undefined || data.data == null) {
                        $(that._obj).remove();
                        return false;
                    }
                    var data2 = "";
                    data2 = that.createAdTotalContent(data.data);
                    if (!!$.trim(data2)) {
                        $(_obj).html(data2);
                        $(_obj).find(".but-ad-item-click").click(function () {
                            that.WirteClickLog(this);
                        });
                        that.resetSizeUseData(_obj, data);
                        that.init(_obj);
                        // window.adsContainer.WirteShowLog()
                    } else {
                        $(_obj).remove();
                        //$(_obj).parent().removeClass("hasad");
                        //$(_obj).empty().hide()
                    }
                } catch (e) {
                    $(_obj).remove();
                }
            }
        })
    },
    removeAd: function () {
        $(this._obj).remove();
    },
    close: function (obj) {
        $(obj).remove();
        this.cache("ad_close", 1, {
            domain: "cnki.net",
            expires: 1,
            session: 1
        })
    },
    cache: function (name, value, options) {
        options = options || {};
        if (value === null) {
            value = "";
            options.expires = -1
        }
        var expires = "";
        var date;
        if (options.expires && (typeof options.expires == "number" || options.expires.toUTCString)) {
            if (typeof options.expires == "number") {
                date = new Date();
                date.setTime(date.getTime() + (options.expires * 60 * 60 * 1000))
            } else {
                date = options.expires
            }
            expires = "; expires=" + date.toUTCString()
        }
        if (window.localStorage || window.sessionStorage) {
            if (typeof value != "undefined") {
                if (options.session) {
                    sessionStorage.setItem(name, value)
                } else {
                    if (date) {
                        var data = {
                            value: value,
                            expirse: date
                        };
                        localStorage.setItem(name, JSON.stringify(data))
                    } else {
                        sessionStorage.setItem(name, value)
                    }
                }
            } else {
                if (!sessionStorage.getItem(name)) {
                    var data = JSON.parse(localStorage.getItem(name));
                    if (data !== null) {
                        if (data.expirse != null && data.expirse < new Date().getTime()) {
                            localStorage.removeItem(name)
                        } else {
                            return data.value
                        }
                    }
                    return null
                } else {
                    return sessionStorage.getItem(name)
                }
            }
        } else {
            if (typeof value != "undefined") {
                var path = options.path ? "; path=" + (options.path) : "";
                var domain = options.domain ? "; domain=" + (options.domain) : "";
                var secure = options.secure ? "; secure" : "";
                document.cookie = [name, "=", encodeURIComponent(value), expires, path, domain, secure].join("")
            } else {
                var cookieValue = null;
                if (document.cookie && document.cookie != "") {
                    var cookies = document.cookie.split(";");
                    for (var i = 0; i < cookies.length; i++) {
                        var cookie = jQuery.trim(cookies[i]);
                        if (cookie.substring(0, name.length + 1) == (name + "=")) {
                            cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                            break
                        }
                    }
                }
                return cookieValue
            }
        }
    },
    WirteShowLog: function () {
        if (this.currentExposure > this.maxExposure) {
            return false;
        }
        this.currentExposure++;
        var ID = "";
        var childList = $(this._obj).find(".carousel-dots");
        if (childList.length == 0) {
            return;
        }
        var childListdot = childList.find(".carousel-dot")
        if (childListdot.length > 0) {
            if ($(this._obj).find(".carousel-dots span.active").length > 0) {
                var liID = $(this._obj).find(".carousel-dots span.active").attr("id").replace("dot", "");
                ID = $(this._obj).find(".carousel-inner .carousel-item:eq(" + (parseInt(liID) - 1) + ") a").attr("dataid")
            } else {
                ID = $(this._obj).find(".carousel-inner .carousel-item a").attr("dataid")
            }
        }
        else {
            ID = $(this._obj).find(".carousel-inner .carousel-item a").attr("dataid")
        }

        if (!ID) {
            return
        }
        var that = this;
        var param = JSON.stringify({ "adWorkerId": ID, "fromUrl": window.location.href });
        //this.consoleLog("WirteShowLog", param);
        $.ajax({
            type: "POST",
            url: this.apiUrl + "app/add/show",
            // dataType: "JSON",
            contentType: 'application/json;charset=UTF-8',
            crossDomain: true,
            xhrFields: {
                widthCreate: true,
            },
            data: param,
            success: function (data) {
                //that.consoleLog(data);
            }
        })
    },
    WirteClickLog: function (obj) {
        var ID = $(obj).attr("dataid");
        if (!ID) {
            return
        }
        var workerId = parseInt(ID);
        if (isNaN(workerId)) {
            return
        }
        var param = JSON.stringify({ adWorkerId: ID, fromUrl: window.location.href });
        //this.consoleLog("WirteClickLog", param);
        var that = this;
        $.ajax({
            type: "POST",
            url: this.apiUrl + "app/add/click",
            // dataType: "JSON",
            contentType: 'application/json;charset=UTF-8',
            crossDomain: true,
            xhrFields: {
                widthCreate: true,
            },
            data: param,
            success: function (data) {
                //that.consoleLog(data);
            }
        })
    },
    parsetIEBrowser: function () {
        var browser = { msie: false, version: 11 };
        var userAgent = navigator.userAgent;
        var rMsie = /(msie\s|trident.*rv:)([\w.]+)/;
        var ua = userAgent.toLowerCase();
        var version = "0";
        var match = rMsie.exec(ua);
        if (match != null) {
            browser.msie = true;
            version = match[2] || "0";
            var num = parseInt(version);
            if (!isNaN(num)) {
                browser.version = num;
            }
        }
        this._browser = browser;
    },
    addcss: function () {
        try {
            var newcsstxt = '.recommenda .carousel{position:relative;overflow:hidden;margin:0 auto;height:100%;font-family:Microsoft YaHei;}.recommenda .carousel-inner{display:flex;flex-wrap:nowrap;transition:transform .5s ease-in-out;min-width:0;height:100%;}.recommenda .carousel-item{flex:0 0 100%;overflow:hidden;position:relative;height:100%;max-height:100%;min-width:0;}.recommenda .carousel-item img,.carousel-item video{max-width:100%;max-height:100%;width:100%;height:100%;object-fit:contain;display:inline-block;vertical-align:middle;min-width:0;}.recommenda .adbutprev-btn,.adbutnext-btn{display:none;position:absolute;top:50%;transform:translateY(-50%);border:none;outline:none;width:14px;height:28px;overflow:hidden;cursor:pointer;background-repeat:no-repeat;}.recommenda .adbutprev-btn{left:0;background-position:left top;background-image:url(data:image/png;base64,)}.recommenda .adbutnext-btn{right:0;background-position:right top;background-image:url(data:image/png;base64,)}.recommenda .carousel-dots{margin-top:-15px;text-align:center;position:absolute;width:100%;}.recommenda .carousel-dot{display:inline-block;width:7px;height:7px;background-color:#ccc;border-radius:50%;margin:0 5px;cursor:pointer;}.recommenda .carousel-dot.active{background-color:#fe6500;}.recommenda .carousel .adbutclose{position:absolute;right:0;z-index:50;}.recommenda .carousel .adbutclose a{padding:3px 7px;float:right;color:white;font-size:12px;text-decoration:none;background-color:rgba(0,0,0,.2);cursor:pointer;border-radius:3px;}';
            if (this._browser.msie && this._browser.version < 11) {
                newcsstxt = '.recommenda .carousel{position:relative;overflow:hidden;margin:0 auto;height:100%;font-family:Microsoft YaHei;width:100%;}.recommenda .carousel-inner{position:relative;display:flex;flex-wrap:nowrap;transition:left .5s ease-in-out;height:100%;}.recommenda .carousel-item{display:inline-block;overflow:hidden;position:relative;height:100%;}.recommenda .carousel-item img,.carousel-item video{max-width:100%;max-height:100%;width:100%;height:100%;object-fit:contain;display:inline-block;vertical-align:middle;min-width:0;}.recommenda .adbutprev-btn,.adbutnext-btn{display:none;position:absolute;top:50%;transform:translateY(-50%);border:none;outline:none;width:14px;height:28px;overflow:hidden;cursor:pointer;background-repeat:no-repeat;}.recommenda .adbutprev-btn{left:0;background-position:left top;background-image:url(data:image/png;base64,)}.recommenda .adbutnext-btn{right:0;background-position:right top;background-image:url(data:image/png;base64,)}.recommenda .carousel-dots{margin-top:-15px;text-align:center;position:absolute;width:100%;}.recommenda .carousel-dot{display:inline-block;width:7px;height:7px;background-color:#ccc;border-radius:50%;margin:0 5px;cursor:pointer;}.recommenda .carousel-dot.active{background-color:#fe6500;}.recommenda .carousel .adbutclose{position:absolute;right:0;z-index:50;}.recommenda .carousel .adbutclose a{padding:3px 7px;float:right;color:white;font-size:12px;text-decoration:none;background-color:rgba(0,0,0,.2);cursor:pointer;border-radius:3px;}';
            }
            var reg1 = new RegExp('.recommenda', "g")
            newcsstxt = newcsstxt.replace(reg1, '#' + this._recommendId);
            if (this._writeCssType == "1") {
                //创建 style
                var hm = document.createElement("style");
                hm.innerHTML = newcsstxt;
                if (document.getElementsByTagName("style").length > 0) {
                    var s = document.getElementsByTagName("style")[0];
                    s.parentNode.insertBefore(hm, s);
                } else {
                    document.head.appendChild(hm);
                }
            } else {
                //write css
                var newcss = '<style type="text/css">' + newcsstxt + '</style>';
                document.writeln(newcss);
            }
        } catch (e) { }
    }
};
