        //             media.style.width = 'auto';
        //             media.style.height = '100%';
        //         }
        //     }
        // });
        // Go to current slide to refresh carousel layout
        // showSlide(currentSlide);
        // });

        // Start autoscroll
        var intervalTimes = 4000;
        if (carouselItems.length > 1) {
            if (that._rotationIntervalTime == 0) {
            } else {
                if (that._rotationIntervalTime > 0) {
                    intervalTimes = that._rotationIntervalTime * 1000;
                }
                var intervalId = setInterval(nextSlide, intervalTimes);
                carousel.addEventListener('mouseenter', function () {
                    clearInterval(intervalId);
                });
                carousel.addEventListener('mouseleave', function () {
                    intervalId = setInterval(nextSlide, intervalTimes);
                });
            }
        }
        if (this.enableClose && closeBtn != null) {
            closeBtn.addEventListener('click', function () {
                carousel.remove();
                //测试
