<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="ApplicationAwareConfigSection" type="Clicknium.Desktop.Uia.app.Customize.ApplicationAwareConfigSection, Clicknium.Desktop.Uia3, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
  </configSections>
  <ApplicationAwareConfigSection>
    <AppCollection>
      <App name="DingTalkShadow" startAtFromPoint="False" useProcessMainWindow="False">
        <IdentityFlag>
          <add constrainStr="ProcessName=DingTalk;ClassName=DuiShadowWnd"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
          <add constrainStr="ControlType=Custom"/>
        </IgnoreFlag>
      </App>
      
      <App name="DingTalk" keyAttritubes="" startAtFromPoint="True" useProcessMainWindow="False">
        <IdentityFlag>
          <add constrainStr="ProcessName=DingTalk"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
          <add constrainStr="ControlType=Custom"/>
        </IgnoreFlag>
        <StopFlag>
        </StopFlag>
      </App>
       <App name="FeiGe" keyAttritubes="" startAtFromPoint="False" useProcessMainWindow="True">
        <IdentityFlag>
          <add constrainStr="ProcessName=飞鸽客服工作台"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
          <add constrainStr="ControlType=Custom"/>
        </IgnoreFlag>
      </App>
       <App name="WeChat" startAtFromPoint="False" useProcessMainWindow="True" reverseSearch="True">
        <IdentityFlag>
          <add constrainStr="ProcessName=WeChat"/>
          <add constrainStr="ProcessName=WeChatApp"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
          <add constrainStr="ControlType=Custom"/>
          <add constrainStr="ControlType=Text;Name="/>
        </IgnoreFlag>
      </App>
       <App name="browser" startAtFromPoint="False" useProcessMainWindow="True">
        <IdentityFlag>
          <add constrainStr="ProcessName=chrome;ControlType=Document"/>
          <add constrainStr="ProcessName=chrome;ControlType=Group"/>
          <add constrainStr="ProcessName=chrome;ControlType=Pane"/>
          <add constrainStr="ProcessName=msedge;ControlType=Document"/>
          <add constrainStr="ProcessName=msedge;ControlType=Group"/>
          <add constrainStr="ProcessName=msedge;ControlType=Pane"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
          <add constrainStr="ControlType=Custom"/>
          <add constrainStr="ControlType=Group"/>
        </IgnoreFlag>
        <NotEnterFlag>
          <add constrainStr="ControlType=Document"/>
        </NotEnterFlag>
      </App>
       <App name="qianniu" startAtFromPoint="False" useProcessMainWindow="False" reverseSearch="True">
        <IdentityFlag>
          <add constrainStr="ProcessName=AliWorkbench"/>
        </IdentityFlag>
        <IgnoreFlag>
          <add constrainStr="ControlType=Pane"/>
          <add constrainStr="ControlType=Window"/>
        </IgnoreFlag>
      </App>
    </AppCollection>
  </ApplicationAwareConfigSection>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/></startup></configuration>
