<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/>
	</startup>
	<appSettings>
		<add key="serilog:minimum-level" value="Debug"/>
		<add key="serilog:using:File" value="Serilog.Sinks.File"/>
		<add key="serilog:write-to:File.shared" value="false"/>
		<add key="serilog:write-to:File.path" value="%LOCALAPPDATA%\Clicknium\Log\ClickniumWebExtensionInstall.log"/>
		<add key="serilog:write-to:File.rollOnFileSizeLimit" value="true"/>
		<add key="serilog:write-to:File.rollingInterval" value="Day"/>
		<add key="serilog:write-to:File.outputTemplate" value="{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u4}] [{SourceContext}] ({ThreadId}) {Message:lj}{NewLine}{Exception}"/>
		<add key="serilog:write-to:File.retainedFileCountLimit" value="10"/>
		<add key="serilog:write-to:File.fileSizeLimitBytes" value="5242880"/>
		<add key="ClientSettingsProvider.ServiceUri" value=""/>
		<add key="ENABLE_FIREFOX_EXTENSION_GUIDE_URL" value="https://www.clicknium.com/documents/tutorial/extensions/firefoxextension"/>
		<add key="EXTENSION_GUIDE_URL" value="https://www.clicknium.com/documents/tutorial/extensions"/>
		<add key="ENABLE_CHROME_EXTENSION_GUIDE_URL" value="https://www.clicknium.com/documents/tutorial/extensions/chromeextension"/>
		<add key="ENABLE_360SE_EXTENSION_GUIDE_URL" value="https://www.clicknium.com/documents/tutorial/extensions"/>
		<add key="ENABLE_NEWEDGE_EXTENSION_GUIDE" value="https://www.clicknium.com/documents/tutorial/extensions/edgeextension"/>
		<add key="PUBLIC_CHROME_EXTENSION_DETAIL_PAGE_URL" value="https://chrome.google.com/webstore/detail/clicknium-recorder/ifnedcgcleipmmolmnhoeemmjnljjgna"/>
		<add key="CHROME_EXTENSION_UPDATE_URL" value="https://clients2.google.com/service/update2/crx"/>
		<add key="CHROME_EXTENSIONS_STORE_URL" value="https://chrome.google.com/webstore/category/extensions"/>
		<add key="FIREFOX_EXTENSIONS_STORE_URL" value="https://addons.mozilla.org/firefox/extensions/"/>
	</appSettings>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Net.Http" publicKeyToken="b03f5f7f11d50a3a" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-4.1.1.0" newVersion="4.1.1.0"/>
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Diagnostics.DiagnosticSource" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>
