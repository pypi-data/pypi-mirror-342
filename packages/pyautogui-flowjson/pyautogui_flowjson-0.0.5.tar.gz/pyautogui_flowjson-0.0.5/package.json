{
  "name": "@zeroer/mcp-server-pyautogui",
  "jspy_identifier": "__jspy_identifier__",
  "version": "0.0.6",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "mcp-server-pyautogui": "dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "src",
    ".python-version",
    "pyproject.toml",
    "uv.lock"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "postinstall": "uv sync",
    "bootstrap": "pnpm i && uv sync",
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc --watch",
    "start": "node $npm_package_bin_pyautogui_mcp",
    "dev": "nodemon --watch src --ext ts --exec 'pnpm build'",
    "inspector": "npx @modelcontextprotocol/inspector node $(pwd)/$npm_package_bin_pyautogui_mcp",
    "test": "chmod +x dist/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@zeroer/general-tools": "^1.1.16",
    "python-shell": "^5.0.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.8.2",
    "@types/node": "^22.14.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.3"
  }
}
