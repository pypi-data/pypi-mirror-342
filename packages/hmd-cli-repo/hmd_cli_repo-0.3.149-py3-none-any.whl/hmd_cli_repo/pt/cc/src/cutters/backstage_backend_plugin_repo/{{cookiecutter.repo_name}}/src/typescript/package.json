{
  "name": "@{{ cookiecutter._npm_scope | lower }}/{{ cookiecutter.repo_name | replace(cookiecutter._org + '-', '') }}",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": {
    "name": "{{ cookiecutter._author }}",
    "email": "{{ cookiecutter._author_email }}"
  },
  "publishConfig": {
    "registry": "{{ cookiecutter._npm_registry_url }}"
  },
  "repository": {
    "type": "git",
    "url": "{{ cookiecutter.__git_url }}"
  },
  "backstage": {
    "role": "backend-plugin"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "tsc && backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "jest --passWithNoTests",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/backend-common": "^0.18.2",
    "@backstage/config": "^1.0.6",
    "@types/express": "*",
    "express": "^4.17.3",
    "express-promise-router": "^4.1.0",
    "winston": "^3.2.1",
    "node-fetch": "^2.6.7",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.22.2",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.2.4",
    "msw": "^0.49.0"
  },
  "resolutions": {
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "files": [
    "dist"
  ]
}
