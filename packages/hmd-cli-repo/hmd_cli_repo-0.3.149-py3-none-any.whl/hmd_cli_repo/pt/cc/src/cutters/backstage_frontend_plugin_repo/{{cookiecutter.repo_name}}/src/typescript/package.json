{
  "name": "@{{ cookiecutter._npm_scope | lower }}/{{ cookiecutter.repo_name | replace(cookiecutter._org + '-', '') }}",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": {
    "name": "{{ cookiecutter._author }}",
    "email": "{{ cookiecutter._author_email }}"
  },
  "publishConfig": {
    "registry": "{{ cookiecutter._npm_registry_url }}"
  },
  "repository": {
    "type": "git",
    "url": "{{ cookiecutter.__git_url }}"
  },
  "backstage": {
    "role": "frontend-plugin"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "tsc && backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "jest --passWithNoTests",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/core-components": "^0.12.4",
    "@backstage/core-plugin-api": "^1.4.0",
    "@backstage/theme": "^0.2.17",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "react-use": "^17.2.4"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.22.2",
    "@backstage/core-app-api": "^1.5.0",
    "@backstage/dev-utils": "^1.0.12",
    "@backstage/test-utils": "^1.2.5",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "*",
    "cross-fetch": "^3.1.5",
    "msw": "^0.49.0"
  },
  "resolutions": {
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "files": [
    "dist"
  ]
}
