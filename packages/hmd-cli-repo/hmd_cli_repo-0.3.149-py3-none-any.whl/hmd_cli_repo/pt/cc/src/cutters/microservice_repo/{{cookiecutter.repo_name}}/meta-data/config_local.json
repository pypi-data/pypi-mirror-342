{
  "loader_config": {
    "base": ["{{ cookiecutter.__module_name }}"]
  },
  "service_loader": "base",
  "operations_modules": [
    "hmd_ms_base.crud_operations",
    "{{ cookiecutter.__module_name }}.operations"
  ],
  "hmd_db_engines": {
    "postgres": {
      "engine_type": "postgres",
      "engine_config": {
        "host": "db",
        "user": "{{ cookiecutter.__module_name }}",
        "password": "{{ cookiecutter.__module_name }}",
        "db_name": "{{ cookiecutter.__module_name }}"
      }
    }
  },
  "hmd_entity_config": { "__default__": { "persistence": ["postgres"] } }
}
